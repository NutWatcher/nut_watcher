<div class="content_post_list">
  <div class="post_title_warp">
    <p class="title"><a><%= post.title %></a></p>
  </div>
  <div class="post_tag_warp">
    <div class="post_date"><span><%= date(post.date, null) %></span></div>
    <div class="post_tag">
      <% if (post.tags && post.tags.length){ %>
        <% post.tags.each(function(tag){ %>
            <span><%= tag.name %></span>
        <%}); %>
      <% } %>
    </div>
  </div>
  <div class="post_content_warp">
    <%- post.excerpt %>
  </div>
</div>
